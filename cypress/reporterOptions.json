{
   "reporterEnabled": "mocha-junit-reporters, mochawesome",
   "mochaJunitReportersReporterOptions": {
      "mochaFile": "cypress/reports/test_results[hash].xml",
      "toConsole": false
   },
   "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports",
      "quiet": true,
      "overwrite": false,
      "html": false,
      "json": true
   }
}
